{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["eslint:recommended"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["prettier"],
  "rules": {
    "quotes": ["warn", "single"],
    "no-duplicate-imports": "warn",
    "no-console": "off",
    "no-unused-vars": "warn",
    "no-undef": "off",
    "no-multiple-empty-lines": "warn"
  }
}
